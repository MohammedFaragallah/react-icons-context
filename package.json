{
	"name": "react-icons-context",
	"version": "1.2.0",
	"description": "icons provider for react projects",
	"repository": "https://github.com/MohammedFaragallah/react-icons-context",
	"license": "MIT",
	"author": "MohammedFaragallah",
	"main": "dist/index.js",
	"module": "dist/index.modern.js",
	"source": "src/index.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "microbundle-crl --no-compress --format modern,cjs",
		"commit": "git-cz",
		"predeploy": "cd example && yarn install && yarn run build",
		"deploy": "gh-pages -d example/build",
		"prepublish": "run-s build",
		"release": "dotenv release-it",
		"start": "microbundle-crl watch --no-compress --format modern,cjs",
		"test": "run-s test:unit test:lint test:build",
		"test:build": "run-s build",
		"test:lint": "eslint .",
		"test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
		"test:watch": "react-scripts test --env=jsdom"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --config .prettierrc --write",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@material-ui/core": "^4.11.0",
		"@types/jest": "^26.0.9",
		"@types/react": "^16.9.46",
		"@types/react-dom": "^16.9.8",
		"@typescript-eslint/eslint-plugin": "^3.9.0",
		"@typescript-eslint/parser": "^3.9.0",
		"babel-eslint": "^10.1.0",
		"cross-env": "^7.0.2",
		"cz-conventional-changelog": "3.2.0",
		"dotenv-cli": "^3.2.0",
		"eslint": "^7.6.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-standard": "^14.1.1",
		"eslint-config-standard-react": "^9.2.0",
		"eslint-config-universe": "^4.0.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-standard": "^4.0.1",
		"gh-pages": "^3.1.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"microbundle-crl": "^0.13.11",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "4.0.0-next.77",
		"release-it": "^13.6.6"
	},
	"peerDependencies": {
		"@material-ui/core": "^4.11.0",
		"react": "^16.13.1"
	},
	"engines": {
		"node": ">=10"
	}
}
